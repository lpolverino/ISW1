	observers  at:  'head'  ifPresent: [:aCollectionOfObservers|
			aCollectionOfObservers  do:[:anObserver|
					anObserver updateObserverHeading: aHeading .
					]
		]!

!methodRemoval: MarsRoverHeadingAndPositionLog #updateObserverHeading: stamp: 'lp 6/4/2023 19:24:58'!
updateObserverHeading: aHeading 
	(aHeading isKindOf: MarsRoverHeadingEast) ifTrue:[ marsRoverLog add: 'East']
		ifFalse:[ (aHeading isKindOf: MarsRoverHeadingWest) ifTrue:[ marsRoverLog add:'West']
			ifFalse:[(aHeading isKindOf: MarsRoverHeadingSouth ) ifTrue:[marsRoverLog add:'South']
				ifFalse:[(aHeading isKindOf: MarsRoverHeadingNorth ) ifTrue:[marsRoverLog add:'North']
					ifFalse:[marsRoverLog add: aHeading ]]]]!

!methodRemoval: MarsRoverHeadingAndPositionLog #updateObserverPosition: stamp: 'lp 6/4/2023 19:24:59'!
updateObserverPosition: aPosition 
	marsRoverLog add: aPosition !

