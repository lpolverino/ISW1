!Perforadora methodsFor: 'scan' stamp: 'lp 5/8/2023 19:51:51' prior: 50612870!
calcularProfundidadDelSueloHasta: tipoDeCapa
	
	| valor acc  |
	
	valor := 0.
	
	valor := tipoDeCapa getCodigoSuelo.
	
	acc := 0.
	
	capasDeSueloDebajo do: [:capa | |altura |
		valor = capa scan ifTrue: [ ^ acc ].
		altura := capa altura.
		altura = nil ifTrue: [ altura := 0 ].
		acc := acc + altura.
	].
	  ^ acc! !

