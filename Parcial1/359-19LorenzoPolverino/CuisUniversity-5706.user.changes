!Perforadora methodsFor: 'scan' stamp: 'lp 5/8/2023 19:04:17' prior: 50606054!
calcularProfundidadDelSuelo
	
	"| acc ix |

	acc := 0.
	ix := 1.
	
	[ix <= capasDeSueloDebajo size] whileTrue: [ | capa altura |
		capa := capasDeSueloDebajo at: ix.
		altura := capa altura.
		altura = nil ifTrue: [ altura := 0 ].
		acc := acc + altura.
		ix := ix + 1.		
	].

	^ acc."
	capasDeSueloDebajo select:[:capa | (capa altura = nil) not]
	
	 ! !
