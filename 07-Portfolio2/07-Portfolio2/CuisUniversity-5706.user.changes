waterConsumption
	
	| index total |
	
	zones ifNil: [zones := OrderedCollection new].
	
	index := 1.
	total := 0.
	
	[index <= zones size] whileTrue: [ | zone |
		zone := zones at: index.
		total := total  + zone waterConsumption .
		index := index + 1.
	].

	^ total.! !

