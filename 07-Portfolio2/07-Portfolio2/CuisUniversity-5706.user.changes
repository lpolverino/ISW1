	zones ifNil: [zones := OrderedCollection new].
	
	index := 1.
	total := 0.
	
	[index <= zones size] whileTrue: [ | zone |
		zone := zones at: index.
		zone type = #ResidentialZone ifTrue: [ total := total + ((9 * wp) * zone getNumberOfCells) ].	
